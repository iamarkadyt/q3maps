textures/rd3ctfq3/cell_pad_armor_red
{
	qer_editorimage textures/rd3ctfq3/cell_pad_armor_red.tga
	qer_trans 0.99
	surfaceparm nodamage
	surfaceparm nolightmap
	surfaceparm nonsolid
	surfaceparm nomarks
	surfaceparm trans
	polygonoffset
